{
  "$schema": "https://raw.githubusercontent.com/JanDeDobbeleer/oh-my-posh/main/themes/schema.json",
  "blocks": [
    {
      "alignment": "left",
      "segments": [
        {
          "foreground": "#45F1C2",
          "style": "plain",
          "template": "┌─[",
          "type": "text"
        },
        {
          "type": "os",
          "style": "plain",
          "foreground": "#FF6B6B",
          "template": "{{ .Icon }}"
        },
        {
          "foreground": "#45F1C2",
          "style": "plain",
          "template": "]──[",
          "type": "text"
        },
        {
          "foreground": "#FFE66B",
          "properties": {
            "style": "folder"
          },
          "style": "plain",
          "template": "{{ .Path }}",
          "type": "path"
        },
        {
          "foreground": "#45F1C2",
          "style": "plain",
          "template": "]",
          "type": "text"
        },
        {
          "foreground": "#FF6B6B",
          "properties": {
            "branch_icon": " ",
            "fetch_status": false
          },
          "style": "plain",
          "template": "──[{{ .HEAD }}]",
          "type": "git"
        }
      ],
      "type": "prompt"
    },
    {
      "alignment": "left",
      "newline": true,
      "segments": [
        {
          "foreground": "#45F1C2",
          "style": "plain",
          "template": "└─❯ ",
          "type": "text"
        }
      ],
      "type": "prompt"
    }
  ],
  "final_space": true,
  "version": 2
}
